# Secrets Template - DO NOT COMMIT WITH REAL VALUES
# Use `kubectl create secret` or sealed-secrets in production
apiVersion: v1
kind: Secret
metadata:
  name: proxie-secrets
  namespace: proxie
type: Opaque
stringData:
  GOOGLE_API_KEY: "your-google-api-key"
  ANTHROPIC_API_KEY: "your-anthropic-api-key"
  CLERK_PUBLISHABLE_KEY: "your-clerk-key"
  CLERK_SECRET_KEY: "your-clerk-secret"
  SENTRY_DSN: "your-sentry-dsn"
  POSTGRES_PASSWORD: "proxie_password"
